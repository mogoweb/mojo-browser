<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE default-apps SYSTEM "gnome-da-list.dtd">
<default-apps>
  <web-browsers>
    <web-browser>
      <name>Mojo Browser</name>
      <executable>browser</executable>
      <command>/opt/apps/org.mojo.browser/files/browser %s</command>
      <icon-name>org.mojo.browser</icon-name>
      <run-in-terminal>false</run-in-terminal>
      <netscape-remote>true</netscape-remote>
      <tab-command>/opt/apps/org.mojo.browser/files/browser %s</tab-command>
      <win-command>/opt/apps/org.mojo.browser/files/browser --new-window %s</win-command>
    </web-browser>
  </web-browsers>
</default-apps>
